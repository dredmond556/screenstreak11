{
  "expo": {
    "name": "ScreenStreak",
    "slug": "screenstreak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      "expo-notifications",
      [
        "expo-build-properties",
        {
          "ios": {
            "infoPlist": {
              "NSUserNotificationsUsageDescription": "ScreenStreak sends helpful reminders and celebrates your achievements to keep you motivated on your digital wellness journey."
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "privacy": {
      "userNotifications": {
        "NSUserNotificationsUsageDescription": "ScreenStreak sends personalized reminders and achievement notifications to support your digital wellness goals."
      }
    }
  }
}