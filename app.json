{
  "expo": {
    "name": "ScreenStreak",
    "slug": "screenstreak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "screenstreak",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.screenstreak.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "ScreenStreak uses the photo library when you choose to share or pick images.",
        "NSPhotoLibraryAddUsageDescription": "ScreenStreak may save images to your photo library when exporting or sharing content.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#111827"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#111827",
      "resizeMode": "contain",
      "dark": {
        "image": "./assets/images/splash.png",
        "backgroundColor": "#0b1220",
        "resizeMode": "contain"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}